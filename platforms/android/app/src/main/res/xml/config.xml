<?xml version='1.0' encoding='utf-8'?>
<widget id="net.app.zeppelin" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BluetoothSerial">
        <param name="android-package" value="com.megster.cordova.BluetoothSerial" />
    </feature>
    <name>zeppelin</name>
    <description>
        Control bluetooth para Zeppelin.
    </description>
    <author>
        Carlos Vizcaya
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/assets/zeppelin.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="fullscreen" value="false" />
    <preference name="Orientation" value="sensorLandscape" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="BackgroundColor" value="#371609" />
</widget>
